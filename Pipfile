[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[requires]
python_version = "3.9"

[scripts]
lint = "flake8"
dev = "uvicorn main:app --workers 4 --port 5000 --reload"
start = "gunicorn --bind 0.0.0.0:$PORT -w 4 -k uvicorn.workers.UvicornWorker main:app"

[packages]
fastapi = "*"
gunicorn = "*"
uvicorn = "*"

[dev-packages]
flake8 = "*"
